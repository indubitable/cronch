@page
@model cronch.Pages.HistoryModel
@{
    ViewData["Title"] = "History";
    ViewData["active"] = "history";
}
<div class="text-center">
    <h1 class="display-4">Job Execution History</h1>
    <p>Showing a maximum of @Model.MaxResults results @(Model.IsDataFiltered ? "based on filters" : "")</p>
    <br />
    <partial name="_ExecutionListPartial" model="Model.Executions" />
</div>
